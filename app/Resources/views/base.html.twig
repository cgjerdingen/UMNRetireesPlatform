<!DOCTYPE html>
<html ng-app="umramember">
    <head>
        <meta charset="UTF-8" />
        <meta name="robots" content="noindex,nofollow" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>{% block title %}UMRA Membership Management{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link href="//fonts.googleapis.com/css?family=Lato:400,700,400italic" rel="stylesheet" type="text/css" />
        <link href="{{ asset('css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('css/font-awesome.min.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('css/typeaheadjs.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('css/2015-tc.css') }}" rel="stylesheet" type="text/css" />
        <!--[if IE 8]>
            <link rel="stylesheet" type="text/css" href="{{ asset('css/ie8.css') }}">
        <![endif]-->
        <link href="{{ asset('css/bootstrap3-wysihtml5.min.css') }}" rel="stylesheet" type="text/css" />
        <link href="{{ asset('css/application.css') }}" rel="stylesheet" type="text/css" />
    </head>
    <body>
        <!-- Skip Links: Give your nav and content elements the appropriate ID attributes -->
        <div id="skipLinks"><a href="#sub-header">Main navigation</a> | <a href="#full-page-wrapper">Main content</a></div>
        <!-- BEGIN HEADER -->
        <header class="umnhf" id="umnhf-h" role="banner">
            <div class="printer"><div class="left"></div><div class="right"><strong>University of Minnesota</strong><br />http://www.umn.edu/<br />612-625-5000</div></div>
            <div class="umnhf" id="umnhf-h-mast">
                <a class="umnhf" id="umnhf-h-logo" href="http://www1.umn.edu/twincities/"><span>Go to the U of M home page</span></a>
                <ul class="umnhf" id="umnhf-h-ql">
                    <li><a href="http://onestop.umn.edu/">One Stop</a></li>
                    <li class="umnhf"><a href="https://www.myu.umn.edu/">MyU <span></span>: For Students, Faculty, and Staff</a></li>
                </ul>
                <!-- Button below is for dropdown toggle, only visible on mobile screens. If using
                a non-dropdown version you can delete this tag -->
                <button class="umnhf" id="umnhf-m-search">Search</button>
            </div>
            <form class="umnhf" id="umnhf-h-search" action="http://search.umn.edu/tc/" method="get" title="Search Websites and People" role="search">
                <label class="umnhf" for="umnhf-h-st">Search</label>
                <input class="umnhf" id="umnhf-h-st" type="text" name="q" />
                <input class="umnhf" id="umnhf-h-sb" type="submit" value="">
            </form>
        </header>
        <!-- END HEADER -->
        <!-- BEGIN OPTIONAL UNIT IDENTIFICATION AND NAVIGATION  -->
        <div class="umnhf toggle display-after-menu-collapse" id="umnhf-alt-um">&#9776; <span class="visually-hidden">Main</span> Menu</div>
        <header id="umnhf-alt" class="umnhf">
            <nav id="umnhf-alt-n" role="navigation">
                {% if app.user %}
                <ul>
                    <li>
                        <a href="http://umra-dev.umn.edu">
                            UMRA Home
                        </a>
                    </li>
                    <li>
                        <a href="{{ path('UMRA_Person_Profile') }}">
                            My Membership
                        </a>
                    </li>

                    {% if is_granted('ROLE_CAN_EDIT_OTHER_HOUSEHOLD') %}
                    <li>
                        <a href="{{ path('UMRA_Household') }}">
                            Members
                        </a>
                    </li>
                    {% endif %}

                    {% if is_granted('ROLE_CAN_EDIT_LUNCHEON') %}
                    <li>
                        <a href="{{ path('UMRA_Luncheon') }}">
                            Luncheons
                        </a>
                    </li>
                    {% endif %}

                    {% if is_granted('ROLE_CAN_EDIT_OTHER_TRANSACTION') %}
                    <li>
                        <a href="{{ path('UMRA_Trans') }}">
                            Transactions
                        </a>
                    </li>
                    {% endif %}

                    {% if is_granted('ROLE_ADMIN') %}
                    <li>
                        <a href="{{ path('UMRA_ContentBlock') }}">
                            Content Blocks
                        </a>
                    </li>
                    {% endif %}

                    {% if is_granted('ROLE_ADMIN') %}
                    <li>
                        <a href="{{ path('sonata_admin_redirect') }}">
                            Administration
                        </a>
                    </li>
                    {% endif %}

                    <li>
                        <a href="{{ path('fos_user_security_logout') }}">
                            Logout
                        </a>
                    </li>
                </ul>
                {% else %}
                <ul>
                    <li>
                        <a href="http://umra-dev.umn.edu">
                            UMRA Home
                        </a>
                    </li>
                    <li>
                        <a href="{{ path('fos_user_security_login') }}">
                            Login
                        </a>
                    </li>
                    <li>
                        <a href="{{ path('UMRA_register') }}">
                            Register
                        </a>
                    </li>
                </ul>
                {% endif %}
            </nav>
            <div class="container">
                <div id="umnhf-alt-sn">
                    <h2>University of Minnesota Retirees Association</h2>
                </div>
            </div>
        </header>
        <!-- END UNIT IDENTIFICATION AND NAVIGATION -->

        <div id="full-page-wrapper">
            <div class="container">
                {% block body %}{% endblock %}
            </div>
        </div>

        <!-- BEGIN OPTIONAL UNIT FOOTER -->
        <section id="umnhf-uf" class="umnhf">
            <h2 class="visually-hidden">Contact Information</h2>
            <address id="umnhf-uf-ci" class="umnhf">
                <p class="umnhf-f-title">University of Minnesota Retirees Association</p>
                <p>200 Oak Street SE, Minneapolis, MN, 55455-2002</p>
                <p>Room 264, Suite 250</p>
                <p><abbr title="Phone Number">P</abbr>: <a href="tel:6126264403">612-626-4403</a></p>
                <p>Email Us: <a href="mailto:umra@umn.edu">umra@umn.edu</a></p>
            </address>
        </section>
        <!-- END OPTIONAL UNIT FOOTER -->
        <!-- BEGIN UofM FOOTER -->
        <footer id="umnhf-f" class="umnhf" role="contentinfo">
            <nav id="umnhf-f-myu">
                <h3 class="umnhf-f-title visually-hidden">For Students, Faculty, and Staff</h3>
                <ul>
                    <li><a href="http://onestop.umn.edu/">One Stop</a></li>
                    <li><a href="https://www.myu.umn.edu/">My U <span></span></a></li>
                </ul>
            </nav>
            <small>&copy; <span id="cdate"></span> Regents of the University of Minnesota. All rights reserved. The University of Minnesota is an equal opportunity educator and employer. <a href="http://privacy.umn.edu">Privacy Statement</a></small>
        </footer>
        <!-- END UofM FOOTER -->
        <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
            <!--[if lt IE 9]>
            <script src="//html5shim.googlecode.com/svn/trunk/html5.js"></script>
            <![endif]-->
        <script src="{{ asset('js/umnhf-2015.js') }}" type="text/javascript"></script>
        <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
        <script src="https://code.jquery.com/jquery.js"></script>
        <!-- Include all compiled plugins (below), or include individual files as needed -->
        <script src="{{ asset('js/bootstrap.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/handlebars-v1.3.0.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/typeahead.bundle.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/peopleSearch.js') }}"></script>
        <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.28/angular.min.js"></script>
        <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.28/angular-route.min.js"></script>
        <script type="text/javascript" src="{{ asset('js/umramember.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/umramember.controllers.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/umramember.directives.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/umramember.filters.js') }}"></script>
        <script type="text/javascript" src="{{ asset('js/umramember.services.js') }}"></script>
        {% block javascripts %}{% endblock %}
    </body>
</html>
