{% extends 'UMRAMemberBundle::nav.html.twig' %}

{% block modulecontent -%}
    <h1>Residence</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Primary</th>
                <td>{{ entity.primary }}</td>
            </tr>
            <tr>
                <th>Address1</th>
                <td>{{ entity.address1 }}</td>
            </tr>
            <tr>
                <th>Address2</th>
                <td>{{ entity.address2 }}</td>
            </tr>
            <tr>
                <th>Address3</th>
                <td>{{ entity.address3 }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ entity.city }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ entity.state }}</td>
            </tr>
            <tr>
                <th>Zip</th>
                <td>{{ entity.zip }}</td>
            </tr>
            <tr>
                <th>Legdistrict</th>
                <td>{{ entity.legdistrict }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ entity.country }}</td>
            </tr>
            <tr>
                <th>Since</th>
                <td>{{ entity.since|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Forseason</th>
                <td>{{ entity.forseason }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('UMRA_Residence') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('UMRA_Residence_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
