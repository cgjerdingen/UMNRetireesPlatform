{% extends 'UMRAMemberBundle::nav.html.twig' %}

{% block modulecontent -%}
    <h1>Role</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Fromdate</th>
                <td>{{ entity.fromdate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Todate</th>
                <td>{{ entity.todate|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ entity.description }}</td>
            </tr>
            <tr>
                <th>Notes</th>
                <td>{{ entity.notes }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('UMRA_Role') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('UMRA_Role_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
