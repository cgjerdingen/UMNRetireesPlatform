{% extends 'UMRAMemberBundle::nav.html.twig' %}

{% block modulecontent -%}
    <h1>Phone</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Phnumber</th>
                <td>{{ entity.phnumber }}</td>
            </tr>
            <tr>
                <th>Phtype</th>
                <td>{{ entity.phtype }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('UMRA_Phone') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('UMRA_Phone_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
