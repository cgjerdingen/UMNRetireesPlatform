<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UMRA\Bundle\MemberBundle\Entity\Person" table="person">
    <indexes>
      <index name="HouseholdID" columns="HouseholdID"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="lastname" type="string" column="Lastname" length="20" nullable="false"/>
    <field name="firstname" type="string" column="Firstname" length="20" nullable="false"/>
    <field name="nickname" type="string" column="Nickname" length="20" nullable="false"/>
    <field name="fullname" type="string" column="Fullname" length="50" nullable="false"/>
    <field name="postalname" type="string" column="Postalname" length="50" nullable="false"/>
    <field name="nametagname" type="string" column="Nametagname" length="20" nullable="false"/>
    <field name="membersince" type="date" column="Membersince" nullable="true"/>
    <field name="utopunit" type="string" column="UTopUnit" length="20" nullable="true"/>
    <field name="udeptequiv" type="string" column="UDeptEquiv" length="20" nullable="true"/>
    <field name="uempltype" type="string" column="UEmplType" length="20" nullable="true"/>
    <field name="utitle" type="string" column="UTitle" length="20" nullable="true"/>
    <field name="ustartdate" type="date" column="UStartdate" nullable="true"/>
    <field name="uretiredate" type="date" column="URetiredate" nullable="true"/>
    <field name="joindate" type="date" column="Joindate" nullable="true"/>
    <field name="deceasedate" type="date" column="Deceasedate" nullable="true"/>
    <field name="activenow" type="string" column="Activenow" length="20" nullable="true"/>
    <field name="postalnews" type="string" column="Postalnews" length="20" nullable="true"/>
    <field name="weburl" type="string" column="Weburl" length="20" nullable="true"/>
    <many-to-one field="householdid" target-entity="Household">
      <join-columns>
        <join-column name="HouseholdID" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
