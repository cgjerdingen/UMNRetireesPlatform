<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UMRA\Bundle\MemberBundle\Entity\Role" table="role">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fromdate" type="date" column="Fromdate" nullable="false"/>
    <field name="todate" type="date" column="Todate" nullable="true"/>
    <field name="description" type="string" column="Description" length="20" nullable="true"/>
    <field name="notes" type="string" column="Notes" length="20" nullable="true"/>
  </entity>
</doctrine-mapping>
