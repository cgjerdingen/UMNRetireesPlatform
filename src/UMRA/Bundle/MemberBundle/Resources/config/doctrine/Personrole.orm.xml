<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UMRA\Bundle\MemberBundle\Entity\Personrole" table="personrole">
    <indexes>
      <index name="PersonID" columns="PersonID"/>
      <index name="RoleID" columns="RoleID"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="roleid" target-entity="Role">
      <join-columns>
        <join-column name="RoleID" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="personid" target-entity="Person">
      <join-columns>
        <join-column name="PersonID" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
